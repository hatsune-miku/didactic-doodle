  "productName": "wal-assistant-lark",
  "version": "0.1.3",
  "identifier": "com.miku.wal",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "wal-assistant-lark",
